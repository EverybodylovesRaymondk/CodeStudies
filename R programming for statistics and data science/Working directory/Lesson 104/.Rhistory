mode <- function(x){
ta <- table(x)
tam <- max(ta)
if (all(ta == tam))
mod <- NA
else
if(is.numeric(x))
mod <- as.numeric(names(ta)[ta == tam])
else
mod <- names(ta)[ta == tam]
return(mod)
}
View(mode)
